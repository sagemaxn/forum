{
  "name": "backend",
  "version": "1.0.0",
  "description": "This is the backend for a simple forum app",
  "main": "server.ts",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "nodemon --exec ts-node server.ts",
    "build:ui": "rm -rf .next && cd ../frontend/ && npm run build && cp -r .next ../backend",
    "deploy": "git push heroku",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy"
  },
  "author": "Sage Maxwell",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.11.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.12.5",
    "apollo-server": "^2.25.2",
    "apollo-server-express": "^2.25.2",
    "apollo-server-plugin-http-headers": "^0.1.4",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-csp-header": "^5.1.0",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.0",
    "reflect-metadata": "^0.1.13",
    "should-send-same-site-none": "^2.0.5",
    "ts-node": "^10.0.0",
    "type-graphql": "^1.1.1",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.4",
    "nodemon": "^2.0.9",
    "ts-node-dev": "^1.1.8"
  }
}
